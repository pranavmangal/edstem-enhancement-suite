{
  "name": "EdStem Enhancement Suite",
  "description": "Improve your Ed experience!",
  "version": "0.1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://edstem.org/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "options.html"
  },
  "permissions": [
    "storage"
  ]
}
